-        }
-        if (arg->flags & FUSE_DONT_MASK) {
-            se->conn.capable |= FUSE_CAP_DONT_MASK;
-        }
-        if (arg->flags & FUSE_FLOCK_LOCKS) {
-            se->conn.capable |= FUSE_CAP_FLOCK_LOCKS;
